<%= render 'partials/header' %>

<br>
<br>

<%= render :partial => "partials/feature", locals: {type: @full_feature} %>

<br>
<%= render 'partials/artwork_admin' %>
<br>
<%= link_to "New Work", new_artwork_path %>
<br>
<br>

<% @artwork.each do |art| %>
	<%= image_tag art.photo.url(:small) %><br>
	<strong>Title: </strong><%= art.title %> <br>
	<strong>Artist: </strong><%= art.artist.fullname if art.artist.present? %><br>
	<strong>Style: </strong>
	<% art.styles.each do |r| %>
		<%= r.title %>
	<% end %>
	<br>
	<strong>JTG#: </strong><%= art.jtg %> <br> <br>
	<strong>Subjects:</strong> <br>
	<% art.subjects.each do |r| %>
		<%= r.title %>
	<% end %>
	<br>
	<strong>Locations:</strong> <br>
	<% art.locations.each do |r| %>
		<%= r.title %>
	<% end %>
	<br>
	<strong>Keywords:</strong> <br>
	<% art.keywords.each do |r| %>
		<%= r.title %>
	<% end %>
	<hr>
<% end %>
